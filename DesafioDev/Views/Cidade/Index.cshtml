@model DesafioDev.Models.CidadeViewModel
<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th scope="col">Cidade</th>
            <th scope="col">Estado</th>
        </tr>
    </thead>

    <tbody>
        @foreach (var cidade in Model.listacidades)
        {
            <tr>
                <td>@cidade.nome</td>
                <td>@cidade.estado</td>
            </tr>
        }

    </tbody>
</table>